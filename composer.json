	"type" : "library",
	"license" : "MIT",
	"keywords" : [
		"zf2",
		"module",
		"zendframework",
		"zf3"
	],
	"homepage" : "https://github.com/rarog/UserRbac",
	"authors" : [{
			"name" : "ojhaujjwal",
			"email" : "bring2uo@gmail.com"
		}, {
			"name" : "Andrej Sinicyn",
			"email" : "rarog@gmail.com"
		}
	],
	"require" : {
		"php" : "^5.6 || ^7.0",
		"zf-commons/zfc-user" : "2.x-dev",
		"zf-commons/zfc-rbac" : "^2.5"
	},
	"require-dev" : {
		"phpunit/phpunit" : "4.*"
